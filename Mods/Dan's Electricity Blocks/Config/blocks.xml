			<property name="DowngradeBlock" value="JunctionBoxBlock_Player"/>
		</block>

		<block name="JunctionBoxStainlessBlock_Player">
			<property name="Extends" value="stainlessSteelMaster"/>
			<property name="CustomIcon" value="controlPanelBase04"/>
